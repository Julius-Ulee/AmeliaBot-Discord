{
  "name": "@distube/soundcloud",
  "version": "1.3.0",
  "description": "Soundcloud plugin for DisTube.js.org",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write \"**/*.{js,ts,json,yml,yaml,md}\"",
    "build": "tsup",
    "build:check": "tsc --noEmit",
    "update": "ncu -u && yarn up '**' -R",
    "_postinstall": "husky install",
    "prepack": "yarn build && pinst -d",
    "postpack": "pinst -e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distubejs/soundcloud.git"
  },
  "keywords": [
    "distube",
    "plugin",
    "soundcloud",
    "discord.js",
    "discord-music-bot"
  ],
  "author": "Skick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distubejs/soundcloud/issues"
  },
  "homepage": "https://github.com/distubejs/soundcloud#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.14.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "commitlint": "^17.3.0",
    "discord.js": "^14.7.1",
    "distube": "^4.0.4",
    "eslint": "^8.28.0",
    "eslint-config-distube": "^1.6.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-jsdoc": "^39.6.4",
    "husky": "^8.0.2",
    "nano-staged": "^0.8.0",
    "pinst": "^3.0.0",
    "prettier": "^2.8.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "soundcloud.ts": "^0.4.9"
  },
  "peerDependencies": {
    "distube": "3||4"
  },
  "nano-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ],
    "*.{json,yml,yaml}": [
      "prettier --write"
    ]
  }
}